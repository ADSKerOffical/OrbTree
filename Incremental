local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/lolpoppyus/Roblox-Lua/master/Pop%20UI%20Lib", true))()

local ui = library:Tab("Orb Tree Incremental")

ui:Textbox("Farm Orbs", function(arg)
for i = 1, arg do
local args = {
    [1] = "Orb"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Reward"):FireServer(unpack(args))
end
end)

ui:Textbox("Farm Golds", function(arg)
for i = 1, arg do
local args = {
    [1] = "Gold"
